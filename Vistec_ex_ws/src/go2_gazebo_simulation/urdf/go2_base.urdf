  <gazebo>
    <plugin filename="ignition-gazebo-imu-system" name="ignition::gazebo::systems::Imu">
    </plugin>
  </gazebo>

  <gazebo reference="imu_link">
    <sensor name="imu_sensor" type="imu">
      <always_on>true</always_on>
      <update_rate>200</update_rate>
      <topic>imu</topic>
      <imu>
        <angular_velocity>
          <x><noise type="none"/></x>
          <y><noise type="none"/></y>
          <z><noise type="none"/></z>
        </angular_velocity>
        <linear_acceleration>
          <x><noise type="none"/></x>
          <y><noise type="none"/></y>
          <z><noise type="none"/></z>
        </linear_acceleration>
      </imu>
    </sensor>
  </gazebo>

